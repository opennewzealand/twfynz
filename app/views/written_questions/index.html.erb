<h1>Listing written_questions</h1>

<table>
  <tr>
    <th>Question</th>
    <th>Answer</th>
    <th>Status</th>
    <th>Question number</th>
    <th>Question year</th>
    <th>Asker</th>
    <th>Askee</th>
    <th>Date asked</th>
    <th>Days late</th>
    <th>Subject</th>
  </tr>

<% for written_question in @written_questions %>
  <tr>
    <td><%=h written_question.question %></td>
    <td><%=h written_question.answer %></td>
    <td><%=h written_question.status %></td>
    <td><%=h written_question.question_number %></td>
    <td><%=h written_question.question_year %></td>
    <td><%=h written_question.asker_id %></td>
    <td><%=h written_question.askee %></td>
    <td><%=h written_question.date_asked %></td>
    <td><%=h written_question.days_late %></td>
    <td><%=h written_question.subject %></td>
    <td><%= link_to 'Show', written_question %></td>
    <td><%= link_to 'Edit', edit_written_question_path(written_question) %></td>
    <td><%= link_to 'Destroy', written_question, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New written_question', new_written_question_path %>
